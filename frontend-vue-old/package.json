{
  "name": "naga-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "mobile:dev": "vite --host",
    "mobile:build": "vite build --mode mobile",
    "mobile:prepare": "npx cap sync",
    "mobile:run:android": "npx cap run android",
    "mobile:run:ios": "npx cap run ios",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:verification": "vitest run --grep verification",
    "test:watch": "vitest --watch",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.ts,.tsx",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,vue,json,css,scss,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,vue,json,css,scss,md}"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.0",
    "@capacitor/core": "^7.4.0",
    "@capacitor/ios": "^7.4.0",
    "@mediapipe/camera_utils": "^0.3.1675466862",
    "@mediapipe/drawing_utils": "^0.3.1675466124",
    "@mediapipe/face_detection": "^0.4.1646425229",
    "@quasar/extras": "^1.17.0",
    "@quasar/vite-plugin": "^1.9.0",
    "better-sqlite3": "^12.2.0",
    "cropperjs": "^2.0.0",
    "dexie": "^4.0.11",
    "exif-js": "^2.3.0",
    "html2canvas": "^1.4.1",
    "pinia": "^3.0.2",
    "qrcode": "^1.5.4",
    "qrcode.vue": "^3.6.0",
    "quasar": "^2.18.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "vite-plugin-pwa": "^1.0.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.7",
    "vue-router": "^4.5.0"
  },
  "overrides": {
    "glob": "^7.2.3",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.0",
    "inflight": "npm:@isaacs/inflight@^1.0.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.0",
    "@eslint/js": "^9.31.0",
    "@intlify/unplugin-vue-i18n": "^6.0.5",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.0",
    "@vitest/ui": "^3.2.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-vue": "^10.3.0",
    "express": "^5.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "sass": "^1.89.1",
    "vite": "^7.0.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.2.0"
  }
}
