{
  "name": "frontend",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "frontend/src",
  "implicitDependencies": ["api-types"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "frontend"
      },
      "dependsOn": ["^build"]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev",
        "cwd": "frontend"
      }
    },
    "mobile:dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run mobile:dev",
        "cwd": "frontend"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "frontend"
      }
    },
    "test:coverage": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test:coverage",
        "cwd": "frontend"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run lint",
        "cwd": "frontend"
      }
    }
  },
  "tags": ["type:app", "scope:frontend", "platform:web", "platform:mobile"]
}